<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:orientation="vertical"
              android:layout_marginRight="@dimen/sizeTiniest"
              android:layout_marginLeft="@dimen/sizeTiniest"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <TextView
        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_top_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/greyDarkName"
        android:textAppearance="@style/TextAppearance.RoboMediumNormal"
        android:singleLine="true"
        android:ellipsize="end"
        tools:text="Next Year"
        tools:textColor="@color/white"/>

    <TextView
        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingBottom="@dimen/sizeNormal"
        android:textColor="@color/greyDarkName"
        android:textAppearance="@style/TextAppearance.RoboBoldTiny"
        android:singleLine="true"
        android:ellipsize="end"
        tools:text="2017"
        tools:textColor="@color/white"/>

    <com.thirdandloom.storyflow.views.OnSwipeStartNotifyRefreshLayout
        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_swipe_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.thirdandloom.storyflow.views.LockedNotifyScrollView
            android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_scroll_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:scrollbars="none">
            <LinearLayout
                android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_container"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_image_view0"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:layout_marginBottom="@dimen/sizeTiny"
                    />

                    <TextView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_story_description_view0"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:layout_gravity="bottom"
                        android:textAppearance="@style/TextAppearance.RoboBlackSmall"
                        android:maxLines="2"
                        android:layout_margin="@dimen/sizeMedium"
                        tools:text="Best party!"
                    />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_image_view1"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:layout_marginBottom="@dimen/sizeTiny"
                    />

                    <TextView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_story_description_view1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:layout_gravity="bottom"
                        android:textAppearance="@style/TextAppearance.RoboBlackSmall"
                        android:maxLines="2"
                        android:layout_margin="@dimen/sizeMedium"
                        tools:text="Best party!"
                    />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_image_view2"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:layout_marginBottom="@dimen/sizeTiny"
                    />

                    <TextView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_story_description_view2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:layout_gravity="bottom"
                        android:textAppearance="@style/TextAppearance.RoboBlackSmall"
                        android:maxLines="2"
                        android:layout_margin="@dimen/sizeMedium"
                        tools:text="Best party!"
                    />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_image_view3"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:layout_marginBottom="@dimen/sizeTiny"
                    />

                    <TextView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_story_description_view3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:layout_gravity="bottom"
                        android:textAppearance="@style/TextAppearance.RoboBlackSmall"
                        android:maxLines="2"
                        android:layout_margin="@dimen/sizeMedium"
                        tools:text="Best party!"
                    />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_image_view4"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:layout_marginBottom="@dimen/sizeTiny"
                    />

                    <TextView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_story_description_view4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:layout_gravity="bottom"
                        android:textAppearance="@style/TextAppearance.RoboBlackSmall"
                        android:maxLines="2"
                        android:layout_margin="@dimen/sizeMedium"
                        tools:text="Best party!"
                    />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_image_view5"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:layout_marginBottom="@dimen/sizeTiny"
                    />

                    <TextView
                        android:id="@+id/adapter_recycler_item_browse_story_small_filled_content_date_bottom_story_description_view5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:layout_gravity="bottom"
                        android:textAppearance="@style/TextAppearance.RoboBlackSmall"
                        android:maxLines="2"
                        android:layout_margin="@dimen/sizeMedium"
                        tools:text="Best party!"
                    />

                </FrameLayout>
            </LinearLayout>

        </com.thirdandloom.storyflow.views.LockedNotifyScrollView>

    </com.thirdandloom.storyflow.views.OnSwipeStartNotifyRefreshLayout>

</LinearLayout>